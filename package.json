{
  "name": "aquiweb_e2e",
  "version": "1.0.0",
  "description": "E2E tests for AquiWeb",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome --headed",
    "test:e2e": "npm run clean && cypress run --browser chrome --headed",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "report:allure": "npm run allure:generate && npm run allure:open",
    "report:cucumber": "node cucumber-html-report.js",
    "clean": "rimraf allure-results allure-report test-results cypress/videos cypress/screenshots",
    "test:all": "npm run clean && npm run test:e2e && npm run report:allure"
  },
  "keywords": [
    "cypress",
    "cucumber",
    "e2e",
    "testing"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^12.0.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.1.5",
    "@shelex/cypress-allure-plugin": "^2.40.0",
    "allure-commandline": "^2.27.0",
    "cypress": "^12.17.4",
    "cypress-multi-reporters": "^1.6.1",
    "esbuild": "^0.19.8",
    "mocha-allure-reporter": "^1.4.0",
    "multiple-cucumber-html-reporter": "^3.4.0",
    "rimraf": "^5.0.5"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/support/step_definitions/**/*.js",
    "nonGlobalStepDefinitions": true,
    "filterSpecs": true,
    "omitFiltered": true,
    "json": {
      "enabled": true,
      "output": "test-results/cucumber-report.json"
    }
  }
}
