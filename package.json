{
  "name": "aquiweb-e2e",
  "version": "1.0.0",
  "description": "AquiWeb E2E Tests",
  "scripts": {
    "test": "cypress run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --env allure=true",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:clear": "rm -r allure-results/ allure-report cypress/screenshots cypress/videos || true",
    "test:allure": "npm run allure:clear && cypress run --env allure=true && npm run allure:report",
    "allure:open": "allure open allure-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "main": "index.js",
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.34.0",
    "allure-commandline": "^2.20.1",
    "cypress": "^12.0.0"
  },
  "dependencies": {
    "cypress": "^12.0.0",
    "@shelex/cypress-allure-plugin": "^2.34.0",
    "allure-commandline": "^2.20.1"
  }
}
